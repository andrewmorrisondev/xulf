generator client {
  provider = "prisma-client-js"
  output   = "../generated"
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String?
  org       String?
  createdAt DateTime @default(now())
}
